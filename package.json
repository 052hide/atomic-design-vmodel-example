{
  "name": "atomic-design-vmodel-example",
  "author": "Hidetoshi Ota (052hide)",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "clear-hard-source-cache": "rm -rf node_modules/.cache/hard-source/",
    "dev": "cross-env HOST=0.0.0.0 PORT=3000 nuxt-ts",
    "build": "nuxt-ts build",
    "build:nocache": "yarn run -s clear-hard-source-cache && yarn run -s build",
    "start": "nuxt-ts start",
    "generate": "nuxt-ts generate",
    "lint:js": "eslint --ext .ts,.js,.vue --ignore-path .eslintignore .",
    "lint": "yarn lint:js",
    "lint:fix": "yarn lint:js --ext .ts,.js,.vue --ignore-path .eslintignore . --fix",
    "test": "jest",
    "storybook": "yarn nuxt-ts storybook",
    "build-storybook": "nuxt-ts storybook build",
    "deploy-storybook": "storybook-to-ghpages -e storybook-static",
    "storybook:deploy": "yarn run -s build-storybook && yarn run -s deploy-storybook"
  },
  "dependencies": {
    "@mdi/font": "^5.6.55",
    "@nuxt/content": "^1.8.1",
    "@nuxt/typescript-runtime": "^2.0.0",
    "@nuxtjs/axios": "^5.12.2",
    "core-js": "^3.6.5",
    "dayjs": "^1.9.1",
    "nuxt": "^2.14.6",
    "vee-validate": "^3.4.0",
    "vue-select": "^3.10.8"
  },
  "devDependencies": {
    "@nuxt/types": "^2.14.6",
    "@nuxt/typescript-build": "^2.0.3",
    "@nuxtjs/eslint-config": "^3.1.0",
    "@nuxtjs/eslint-config-typescript": "^3.0.0",
    "@nuxtjs/eslint-module": "^2.0.0",
    "@nuxtjs/storybook": "^2.2.1",
    "@nuxtjs/tailwindcss": "^3.1.0",
    "@storybook/addon-knobs": "^6.0.25",
    "@storybook/storybook-deployer": "^2.8.6",
    "@types/vue-select": "^2.5.0",
    "@vue/test-utils": "^1.1.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.3.0",
    "cross-env": "^7.0.2",
    "eslint": "^7.10.0",
    "eslint-config-prettier": "^6.12.0",
    "eslint-plugin-nuxt": "^1.0.0",
    "eslint-plugin-prettier": "^3.1.4",
    "jest": "^26.4.2",
    "node-sass": "^4.14.1",
    "nuxt-property-decorator": "^2.8.8",
    "prettier": "^2.1.1",
    "sass-loader": "^10.0.2",
    "ts-jest": "^26.4.1",
    "vue-jest": "^3.0.4",
    "vuex-module-decorators": "^1.0.1"
  }
}
